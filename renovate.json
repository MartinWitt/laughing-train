{
  "extends": [
    "config:recommended",
    "helpers:pinGitHubActionDigests"
  ],
  "semanticCommitType": "deps:",
  "semanticCommits": "auto",
  "packageRules": [
    {
      "matchUpdateTypes": [
        "bump",
        "digest",
        "lockFileMaintenance",
        "major",
        "minor",
        "patch",
        "pin",
        "pinDigest",
        "replacement"
       ],

      "automerge": true
    }
  ]
}